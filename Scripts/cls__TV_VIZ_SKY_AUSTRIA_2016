        b__VisibleDSS   = cBool(GetIniOption("VISIBLE_DSS", "false"))         ' DSS allgemein
        b__VisibleClock = cBool(GetIniOption("VISIBLE_CLOCK", "False"))       ' DSS mit Rolling Time
