    Public Sub GetPreview(ByVal PrevData, ByVal intFrame, ByVal sScene)
        Dim sCmd
        'If tw_preview.Visible Then
            If IsArray(PrevData) Then
               iCount = UBound(PrevData)
               For i = 0 to iCount
                   strPrevCmd = strPrevCmd & PrevData(i) & " "
