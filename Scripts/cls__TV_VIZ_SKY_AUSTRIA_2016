    function GetPosABC(pos)
        select case lcase(pos)
        case "goalkeeper"
             GetPosABC="TOR"
       case "defender"
             GetPosABC="DEF"
       case "forward"
             GetPosABC="OFF"
        case default
            GetPosABC=""
        end select
    end function

