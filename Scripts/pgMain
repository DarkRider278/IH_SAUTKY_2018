sub pgMain_bt_show_vorlauflogoClick (Sender)

        dim k,i

        'If TSP.LoadSQLData("SELECT * FROM TEAMS WHERE league_id='" & TSP.GetRound(TSP_LEAGUE_ID) & "'") Then
        If TSP.LoadSQLData("SELECT * FROM TEAMS") Then
            If TSP.SetSQLPos("First") Then
